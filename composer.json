{
  "name": "mage2playground/boilerplate",
  "description": "Dev environment to play with Magento 2, PWA Studio, Vue Storefront",
  "type": "project",
  "license": [
    "MIT",
    "OSL-3.0",
    "AFL-3.0"
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "magento/product-community-edition": "~2.3.0",
    "divanteltd/vue-storefront": "~1.8.0",
    "divanteltd/vue-storefront-api": "~1.8.0",
    "divanteltd/mage2vuestorefront": "~1.8.0",
    "magento-research/pwa-studio": "~2.0.0-RC"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2.13.0",
    "lusitanian/oauth": "~0.8.10",
    "magento/magento2-functional-testing-framework": "2.3.9",
    "pdepend/pdepend": "2.5.2",
    "phpmd/phpmd": "@stable",
    "phpunit/phpunit": "~6.5.0",
    "sebastian/phpcpd": "~3.0.0",
    "squizlabs/php_codesniffer": "3.3.1"
  },
  "conflict": {
    "gene/bluefoot": "*"
  },
  "autoload": {
    "psr-4": {
      "Magento\\Framework\\": "lib/internal/Magento/Framework/",
      "Magento\\Setup\\": "setup/src/Magento/Setup/",
      "Magento\\": "app/code/Magento/",
      "Zend\\Mvc\\Controller\\": "setup/src/Zend/Mvc/Controller/"
    },
    "psr-0": {
      "": [
        "app/code/",
        "generated/code/"
      ]
    },
    "files": [
      "app/etc/NonComposerComponentRegistration.php"
    ],
    "exclude-from-classmap": [
      "**/dev/**",
      "**/update/**",
      "**/Test/**"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
      "Magento\\Tools\\": "dev/tools/Magento/Tools/",
      "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
      "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
      "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
    }
  },
  "version": "0.0.1",
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    },
    {
      "type": "package",
      "package": {
        "name": "divanteltd/vue-storefront",
        "version": "v1.8.2",
        "source": {
          "url": "https://github.com/DivanteLtd/vue-storefront.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "divanteltd/vue-storefront-api",
        "version": "v1.8.1",
        "source": {
          "url": "https://github.com/DivanteLtd/vue-storefront-api.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "divanteltd/mage2vuestorefront",
        "version": "v1.8.0",
        "source": {
          "url": "https://github.com/DivanteLtd/mage2vuestorefront.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "magento-research/pwa-studio",
        "version": "v2.0.0-rc.19",
        "source": {
          "url": "https://github.com/magento-research/pwa-studio.git",
          "type": "git",
          "reference": "release/2.0"
        }
      }
    }
  ],
  "extra": {
    "magento-force": "override"
  }
}
